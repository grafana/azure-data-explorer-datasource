{
  "name": "grafana-azure-data-explorer-datasource",
  "version": "7.2.1",
  "description": "Grafana data source for Azure Data Explorer",
  "scripts": {
    "build": "webpack -c ./webpack.config.ts --env production",
    "dev": "webpack -w -c ./webpack.config.ts --env development",
    "e2e:playwright": "yarn playwright test",
    "e2e:playwright:ui": "yarn playwright test --ui",
    "e2e:playwright:report": "yarn playwright show-report",
    "i18n-extract": "i18next --config src/locales/i18next-parser.config.cjs",
    "lint": "eslint --cache .",
    "lint:fix": "yarn run lint --fix && prettier --write --list-different .",
    "server": "docker compose up --build",
    "sign": "npx --yes @grafana/sign-plugin@latest",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "test:coverage": "jest --coverage",
    "test:coverage:changes": "jest --coverage --changedSince=origin/main",
    "typecheck": "tsc --noEmit",
    "generate-release-notes": "PREV_TAG=$(git tag | tail -n 1) && gh api --method POST /repos/grafana/azure-data-explorer-datasource/releases/generate-notes -f tag_name=v${npm_package_version} -f target_commitish=main -f previous_tag_name=${PREV_TAG} | jq -r .body",
    "spellcheck": "cspell -c cspell.config.json \"**/*.{ts,tsx,js,go,md,mdx,yml,yaml,json,scss,css}\"",
    "e2e": "playwright test"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "author": "Grafana Labs",
  "license": "Apache",
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@eslint/compat": "^2.0.0",
    "@grafana/eslint-config": "^8.0.0",
    "@grafana/plugin-e2e": "^3.1.0",
    "@grafana/plugin-meta-extractor": "^0.11.0",
    "@grafana/tsconfig": "^2.0.1",
    "@playwright/test": "1.57.0",
    "@stylistic/eslint-plugin-ts": "^4.4.1",
    "@swc/core": "^1.3.90",
    "@swc/helpers": "^0.5.0",
    "@swc/jest": "^0.2.26",
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/chance": "^1.1.6",
    "@types/debounce-promise": "^3.1.9",
    "@types/glob": "^8.0.0",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.17",
    "@types/node": "^22.13.2",
    "@types/prismjs": "1.26.5",
    "@types/react-router-dom": "^5.3.3",
    "@types/testing-library__jest-dom": "5.14.9",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.51.0",
    "chance": "^1.1.12",
    "copy-webpack-plugin": "^12.0.2",
    "cspell": "9.4.0",
    "css-loader": "^6.7.3",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-deprecation": "^3.0.0",
    "eslint-plugin-jsdoc": "^61.5.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-webpack-plugin": "^5.0.2",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "glob": "^10.2.7",
    "i18next-parser": "9.3.0",
    "identity-obj-proxy": "3.0.0",
    "imports-loader": "^5.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "monaco-editor": "0.34.1",
    "node-notifier": "^10.0.1",
    "prettier": "^3.2.5",
    "prismjs": "1.30.0",
    "react-select-event": "^5.5.1",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.72.0",
    "sass-loader": "14.1.1",
    "semver": "^7.7.2",
    "style-loader": "3.3.4",
    "swc-loader": "^0.2.3",
    "terser-webpack-plugin": "^5.3.16",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.5.4",
    "uuid": "^13.0.0",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "webpack-livereload-plugin": "^3.0.2",
    "webpack-subresource-integrity": "^5.1.0",
    "webpack-virtual-modules": "^0.6.2"
  },
  "dependencies": {
    "@emotion/css": "11.11.2",
    "@grafana/azure-sdk": "^0.0.7",
    "@grafana/data": "11.6.8",
    "@grafana/i18n": "12.3.0",
    "@grafana/llm": "^0.14.1",
    "@grafana/plugin-ui": "^0.12.0",
    "@grafana/runtime": "11.6.8",
    "@grafana/schema": "11.6.8",
    "@grafana/ui": "11.6.8",
    "debounce-promise": "^3.1.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-use": "^17.6.0",
    "tslib": "2.6.2"
  },
  "packageManager": "yarn@4.9.1"
}
