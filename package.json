{
  "name": "grafana-azure-data-explorer-datasource",
  "version": "4.1.9",
  "description": "Grafana data source for Azure Data Explorer",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "e2e": "yarn cypress install && yarn grafana-e2e run",
    "e2e:open": "grafana-e2e open",
    "e2e:update": "yarn cypress install && yarn grafana-e2e run --update-screenshots",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "yarn lint --fix",
    "server": "docker-compose up --build",
    "sign": "grafana-toolkit plugin:sign",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --maxWorkers 4",
    "test:coverage": "jest --coverage",
    "test:coverage:changes": "jest --coverage --changedSince=origin/main",
    "typecheck": "tsc --noEmit",
    "watch": "grafana-toolkit plugin:dev --watch",
    "generate-release-notes": "PREV_TAG=$(git tag | tail -n 1) && gh api --method POST /repos/grafana/azure-data-explorer-datasource/releases/generate-notes -f tag_name=v${npm_package_version} -f target_commitish=main -f previous_tag_name=${PREV_TAG} | jq -r .body"
  },
  "author": "Grafana Labs",
  "license": "Apache",
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@grafana/e2e": "9.1.2",
    "@grafana/e2e-selectors": "9.1.2",
    "@grafana/eslint-config": "^2.5.0",
    "@grafana/tsconfig": "^1.2.0-rc1",
    "@swc/core": "^1.2.144",
    "@swc/helpers": "^0.3.6",
    "@swc/jest": "^0.2.20",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^13.1.9",
    "@types/chance": "^1.1.2",
    "@types/debounce-promise": "^3.1.2",
    "@types/glob": "^8.0.0",
    "@types/grafana": "github:CorpGlory/types-grafana.git",
    "@types/jest": "^27.4.1",
    "@types/lodash": "latest",
    "@types/node": "^17.0.19",
    "@types/prismjs": "1.26.0",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "chance": "^1.1.7",
    "copy-webpack-plugin": "^10.0.0",
    "css-loader": "6.7.1",
    "cypress": "9.3.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsdoc": "^39.3.24",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^3.1.1",
    "fork-ts-checker-webpack-plugin": "^7.2.0",
    "glob": "^8.0.3",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.5.0",
    "monaco-editor": "0.17.0",
    "node-notifier": "^10.0.1",
    "prettier": "^2.5.0",
    "prismjs": "1.29.0",
    "react-select-event": "^5.2.0",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.55.0",
    "sass-loader": "13.1.0",
    "style-loader": "3.3.1",
    "swc-loader": "^0.1.15",
    "ts-node": "^10.5.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.4.0",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2",
    "webpack-livereload-plugin": "^3.0.2"
  },
  "dependencies": {
    "@emotion/css": "^11.1.3",
    "@grafana/data": "9.2.1",
    "@grafana/experimental": "0.0.2-canary.35",
    "@grafana/runtime": "9.2.1",
    "@grafana/ui": "9.2.1",
    "debounce-promise": "^3.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-use": "^15.3.3",
    "tslib": "^2.3.1"
  },
  "resolutions": {
    "**/@testing-library/dom": "7.31.2"
  }
}
