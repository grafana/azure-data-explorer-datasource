{
  "name": "grafana-azure-data-explorer-datasource",
  "version": "5.0.7",
  "description": "Grafana data source for Azure Data Explorer",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "e2e": "yarn exec cypress install && yarn exec grafana-e2e run",
    "e2e:open": "grafana-e2e open",
    "e2e:update": "yarn exec cypress install && yarn exec grafana-e2e run --update-screenshots",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "yarn run lint --fix",
    "server": "docker-compose up --build",
    "sign": "npx --yes @grafana/sign-plugin@latest",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "test:coverage": "jest --coverage",
    "test:coverage:changes": "jest --coverage --changedSince=origin/main",
    "typecheck": "tsc --noEmit",
    "generate-release-notes": "PREV_TAG=$(git tag | tail -n 1) && gh api --method POST /repos/grafana/azure-data-explorer-datasource/releases/generate-notes -f tag_name=v${npm_package_version} -f target_commitish=main -f previous_tag_name=${PREV_TAG} | jq -r .body",
    "spellcheck": "cspell -c cspell.config.json \"**/*.{ts,tsx,js,go,md,mdx,yml,yaml,json,scss,css}\""
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "author": "Grafana Labs",
  "license": "Apache",
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@grafana/e2e": "10.4.1",
    "@grafana/e2e-selectors": "10.4.1",
    "@grafana/eslint-config": "^7.0.0",
    "@grafana/tsconfig": "^1.3.0-rc1",
    "@swc/core": "^1.3.90",
    "@swc/helpers": "^0.5.0",
    "@swc/jest": "^0.2.26",
    "@testing-library/jest-dom": "6.1.4",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/chance": "^1.1.6",
    "@types/debounce-promise": "^3.1.9",
    "@types/glob": "^8.0.0",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.194",
    "@types/node": "^20.8.7",
    "@types/prismjs": "1.26.4",
    "@types/react-router-dom": "^5.2.0",
    "@types/testing-library__jest-dom": "5.14.8",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "chance": "^1.1.12",
    "copy-webpack-plugin": "^12.0.2",
    "cspell": "8.15.3",
    "css-loader": "^6.7.3",
    "cypress": "13.15.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-plugin-jsdoc": "^50.2.5",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.36.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-webpack-plugin": "^4.0.1",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "glob": "^10.2.7",
    "identity-obj-proxy": "3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "monaco-editor": "0.34.1",
    "node-notifier": "^10.0.1",
    "prettier": "^3.2.5",
    "prismjs": "1.29.0",
    "react-select-event": "^5.2.0",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.72.0",
    "sass-loader": "14.1.1",
    "style-loader": "3.3.4",
    "swc-loader": "^0.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.4.3",
    "uuid": "^10.0.0",
    "webpack": "^5.86.0",
    "webpack-cli": "^5.1.4",
    "webpack-livereload-plugin": "^3.0.2"
  },
  "dependencies": {
    "@emotion/css": "11.11.2",
    "@grafana/azure-sdk": "0.0.3",
    "@grafana/data": "10.4.1",
    "@grafana/experimental": "^1.7.0",
    "@grafana/runtime": "10.4.1",
    "@grafana/schema": "10.4.1",
    "@grafana/ui": "10.4.1",
    "debounce-promise": "^3.1.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-use": "^17.5.1",
    "tslib": "2.6.2"
  },
  "resolutions": {
    "**/@testing-library/dom": "7.31.2"
  },
  "packageManager": "yarn@1.22.19"
}
